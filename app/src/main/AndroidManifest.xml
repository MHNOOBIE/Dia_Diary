<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mahdi.dia_diary">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register_Activity" />
        <activity android:name=".PatientModule_Activity" />
        <activity android:name=".GlucoseTracker_Activity" />
        <activity android:name=".LogEntry_Activity" />
        <activity android:name=".GlucoseInput_Acitivty" />
        <activity android:name=".InsulinInput_Activity" />
        <activity android:name=".DiaTracker_Activity" />
        <activity android:name=".InsulinTracker_Activity" />
        <activity android:name=".MixedTracker_Activity" />
        <activity android:name=".UploadReport_Activity" />
        <activity android:name=".ShowReports_Activity" />
        <activity android:name=".PatientRoot_Activity" />
        <activity android:name=".InteractWithDoctor_Activity" />
        <activity android:name=".DoctorDetails_Activity"></activity>
    </application>

</manifest>